    "fp-ts": "^2.12.1",
    "reflect-metadata": "^0.1.13",
    "runtypes": "^6.5.1",
    "rxjs": "^7.0.0",
    "slug": "^5.3.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
