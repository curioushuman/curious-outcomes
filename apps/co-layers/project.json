{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/co-layers/**/*.ts"]
      }
    },
    "list": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "list"
      },
      "outputs": ["{options.outputPath}"]
    },
    "synth": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "synth"
      },
      "outputs": ["{options.outputPath}"]
    },
    "bootstrap": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "bootstrap"
      }
    },
    "deploy": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "deploy"
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": [{
        "target": "build",
        "projects": "dependencies"
      }]
    },
    "destroy": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "destroy"
      }
    },
    "diff": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "diff"
      },
      "dependsOn": [{
        "target": "build",
        "projects": "dependencies"
      }]
    },
    "metadata": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "metadata"
      }
    },
    "context": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "context"
      }
    },
    "docs": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "docs"
      }
    },
    "doctor": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/co-layers",
        "command": "doctor"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/co-layers"],
      "options": {
        "jestConfig": "apps/co-layers/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:layers"],
  "implicitDependencies": ["co-common", "co-courses", "error-factory", "fp-ts-utils", "loggable"]
}
