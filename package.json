{
  "name": "curious-outcomes",
  "version": "0.0.1",
  "description": "Using community-supported learning to drive the most necessary outcomes.",
  "productName": "Curious Outcomes",
  "author": "Mike Kelly",
  "license": "MIT",
  "scripts": {
    "--ADMIN": "SCRIPTS--",
    "lint": "nx run-many --all --target=lint --parallel",
    "test": "nx run-many --all --target=test --parallel",
    "--DEPLOYMENT": "SCRIPTS--",
    "deploy-local": "nx run-many --parallel=1 --target=deploy-local --projects=co-layers,courses,api-public,api-admin",
    "destroy-local": "nx run-many --parallel=1 --target=destroy-local --projects=api-public,api-admin,courses,co-layers",
    "deploy-hybrid": "nx run-many --parallel=1 --target=deploy-local --projects=courses,api-public,api-admin",
    "destroy-hybrid": "nx run-many --parallel=1 --target=destroy-local --projects=api-public,api-admin,courses",
    "deploy": "nx run-many --verbose --parallel=1 --target=deploy --projects=co-layers,courses,api-public,api-admin",
    "destroy": "nx run-many --verbose --parallel=1 --target=destroy --projects=api-public,api-admin,courses",
    "--SUPPORTING": "SCRIPTS--",
    "set-env-develop": "cat .env | awk '{sub(/hybrid|local/,\"develop\")}1' | tee .env-tmp && mv .env-tmp .env",
    "predeploy-local": "npm run set-env-local && npm run build-localstack",
    "set-env-local": "cat .env | awk '{sub(/hybrid|develop/,\"local\")}1' | tee .env-tmp && mv .env-tmp .env",
    "predeploy-hybrid": "npm run set-env-hybrid && build-localstack && npm run add-live-layer-permissions && sleep 2",
    "set-env-hybrid": "cat .env | awk '{sub(/develop|local/,\"hybrid\")}1' | tee .env-tmp && mv .env-tmp .env",
    "build-localstack": "npm run start-localstack && npm run bootstrap-localstack && npm run build-support",
    "start-localstack": "docker-compose up &",
    "bootstrap-localstack": "sleep 10 && node_modules/.bin/cdklocal bootstrap aws://000000000000/ap-southeast-2",
    "build-support": "nx run-many --parallel=3 --target=build --projects=loggable,fp-ts-utils,co-common,co-courses,co-cdk-utils",
    "add-live-layer-permissions": "nx run co-layers:add-live-layer-permissions",
    "postdestroy-local": "npm run stop-localstack",
    "postdestroy-hybrid": "npm run remove-live-layer-permissions && npm run stop-localstack",
    "remove-live-layer-permissions": "nx run co-layers:remove-live-layer-permissions",
    "stop-localstack": "pkill docker-compose up"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/cqrs": "^8.0.5",
    "@nestjs/platform-express": "^8.0.0",
    "aws-cdk-lib": "^2.30.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.1.43",
    "fp-ts": "^2.12.1",
    "reflect-metadata": "^0.1.13",
    "runtypes": "^6.5.1",
    "rxjs": "^7.0.0",
    "slug": "^5.3.0",
    "source-map-support": "^0.5.20",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.124.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "14.3.6",
    "@nrwl/eslint-plugin-nx": "14.3.6",
    "@nrwl/jest": "14.3.6",
    "@nrwl/linter": "14.3.6",
    "@nrwl/nest": "14.3.6",
    "@nrwl/node": "14.3.6",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/web": "14.3.6",
    "@nrwl/workspace": "14.3.6",
    "@otterdev/nx-cdk": "^0.0.14",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/slug": "^5.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
    "@typescript-eslint/parser": "~5.24.0",
    "aws-cdk": "^2.30.0",
    "aws-cdk-local": "^2.15.0",
    "babel-jest": "27.5.1",
    "cdkp": "^0.7.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.48",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^7.0.0",
    "jest": "27.5.1",
    "jest-cucumber": "^3.0.1",
    "nx": "14.3.6",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "run-script-webpack-plugin": "^0.1.1",
    "swagger-cli": "^4.0.4",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2",
    "webpack": "^5.73.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
