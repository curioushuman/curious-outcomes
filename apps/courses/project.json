{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/courses/**/*.ts"]
      }
    },
    "list": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "list"
      },
      "outputs": ["{options.outputPath}"]
    },
    "build": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "synth"
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": [{
        "target": "build",
        "projects": "dependencies"
      }]
    },
    "synth": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "synth"
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": [{
        "target": "build",
        "projects": "dependencies"
      }]
    },
    "bootstrap": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "bootstrap"
      }
    },
    "deploy": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "deploy"
      },
      "outputs": ["{options.outputPath}"],
      "dependsOn": [{
        "target": "deploy",
        "projects": "dependencies"
      }]
    },
    "destroy": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "destroy"
      }
    },
    "diff": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "diff"
      },
      "dependsOn": [{
        "target": "build",
        "projects": "dependencies"
      }]
    },
    "metadata": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "metadata"
      }
    },
    "context": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "context"
      }
    },
    "docs": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "docs"
      }
    },
    "doctor": {
      "executor": "@otterdev/nx-cdk:run",
      "options": {
        "outputPath": "dist/apps/courses",
        "command": "doctor"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/courses"],
      "options": {
        "jestConfig": "apps/courses/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["co-layers"]
}
